{
  "name": "TheEyeDesktop",
  "productName": "TheEye",
  "version": "0.1.0",
  "description": "An electron wrapper for theeye app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=Interactar --version-string.FileDescription=Interactar --version-string.ProductName=\"TheEye - Beta\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/cgastrell/theeye-electron",
  "keywords": [],
  "author": "Interactar SRL",
  "license": "",
  "devDependencies": {
    "electron": "~1.8.2"
  },
  "dependencies": {
    "electron-window-state": "^4.1.1"
  }
}
